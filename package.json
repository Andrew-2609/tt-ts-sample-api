{
  "name": "tt-ts-sample-api",
  "version": "1.0.0",
  "description": "TODO",
  "main": "index.js",
  "scripts": {
    "transpile": "babel src --extensions \".js,.ts\" --out-dir dist --ignore \"src/@types/**\" --copy-files --no-copy-ignored",
    "build": "rimraf dist && yarn transpile",
    "start": "NODE_ENV=production node dist/main/server.js",
    "up": "docker-compose up -d && docker logs -f tt_ts_sample_api",
    "down": "docker-compose down",
    "up:build": "docker-compose up --build -d && docker logs -f tt_ts_sample_api",
    "start:local": "ts-node-dev -r tsconfig-paths/register --respawn --transpile-only --ignore-watch node_modules src/main/server.ts",
    "up:test": "docker-compose --env-file .env.test -f docker-compose.testing.yml up -d",
    "down:test": "docker-compose -f docker-compose.testing.yml down",
    "test": "docker exec tt_ts_sample_api_test node --expose-gc --no-compilation-cache ./node_modules/.bin/jest --runInBand --detectOpenHandles --colors --forceExit",
    "test:unit": "yarn test -c jest-config-unit.js",
    "test:coverage": "yarn test --coverage",
    "test:pattern": "yarn test --testPathPattern"
  },
  "author": {
    "name": "Andrew Silva",
    "email": "andrew.agoravai@gmail.com",
    "url": "https://www.linkedin.com/in/andrew-2609/?locale=en_US"
  },
  "license": "MIT",
  "dependencies": {
    "@aws-sdk/client-sqs": "3.398.0",
    "compression": "1.7.4",
    "express": "4.17.3",
    "express-async-errors": "3.1.1",
    "http-status-codes": "^2.3.0",
    "pino": "8.8.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.21.5",
    "@babel/core": "^7.22.1",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-decorators": "^7.22.3",
    "@babel/preset-env": "^7.22.4",
    "@babel/preset-typescript": "^7.21.5",
    "@types/compression": "^1.7.4",
    "@types/express": "^4.17.12",
    "@types/jest": "^29.5.2",
    "@types/node": "^18.7.9",
    "@types/pino": "7.0.5",
    "@typescript-eslint/eslint-plugin": "^4.17.0",
    "@typescript-eslint/parser": "^4.17.0",
    "babel-plugin-module-resolver": "^5.0.0",
    "babel-plugin-transform-typescript-metadata": "^0.3.2",
    "eslint": "^7.22.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard-with-typescript": "^20.0.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.3.1",
    "jest": "^29.5.0",
    "ts-jest": "^29.1.0",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "3.10.1",
    "typescript": "^4.5.5"
  }
}
